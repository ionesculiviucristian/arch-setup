## Profiles

<% profiles.forEach(function(profile) { -%>
### <%- profile.label %>

<% profile.groups.forEach(function(group) { -%>
#### <%- group.label %>

<% group.packages.sort(sortByLabel).forEach(function(pkg) { -%>
- [<%- pkg.label %>](<%- pkg.url %>): <%- pkg.description %>
<% if (pkg.source_url) { %>  - [Source](<%- pkg.source_url %>)
<% } -%>
<% }); -%>

<% }); -%>
<% if (profile.plugins.length > 0) { -%>
#### Browser plugins

<% profile.plugins.forEach(function(browser) { -%>
##### <%- browser.package.label %> plugins

<% browser.categories.forEach(function(category) { -%>
###### <%- category.label %>

<% category.plugins.sort(sortByName).forEach(function(plugin) { -%>
- [<%- plugin.name %>](<%- browser.url %>/<%- plugin.id %>): <%- plugin.description %>
<% (plugin.extra || []).sort(sortByName).forEach(function(extra) { -%>
  - [<%- extra.name %>](<%- extra.url || `${browser.url}/${extra.id}` %>): <%- extra.description %>
<% }); -%>
<% }); -%>

<% }); -%>
<% }); -%>
<% } -%>
<% }); -%>
